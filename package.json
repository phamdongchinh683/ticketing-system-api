{
  "name": "backend-fastify-setting",
  "type": "module",
  "packageManager": "yarn@4.11.0",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec tsx src/app/api.ts",
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc",
    "prestart": "yarn build",
    "start": "node dist/app/api.js",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "migration:create": "kysely migrate:make",
    "migration:down": "kysely migrate:down",
    "migrate": "kysely migrate:latest"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@fastify/compress": "^8.0.0",
    "@fastify/cors": "^11.2.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "D": "^1.0.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "fast-jwt": "^6.1.0",
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^6.1.0",
    "kysely": "^0.28.11",
    "kysely-ctl": "^0.20.0",
    "lodash": "^4.17.21",
    "openapi-types": "^12.1.3",
    "pg": "^8.16.3",
    "qs": "^6.14.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/lodash": "^4",
    "@types/node": "^25.0.3",
    "@types/pg": "^8",
    "@types/qs": "^6",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-perfectionist": "4.15.1",
    "eslint-plugin-prettier": "5.5.4",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  }
}
